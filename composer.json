{
    "name": "thtmorais/yii2-icons",
    "description": "Assets icons for Yii PHP Framework",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii2-extension", "yii2-icons", "feather-icons", "flag-icons"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/thtmorais/yii2-icons/issues",
        "source": "https://github.com/thtmorais/yii2-icons"
    },
    "authors": [
        {
            "name": "Matheus Evangelista Morais",
            "email": "thtmorais@hotmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.0",
        "npm-asset/feather-icons": "4.29.2",
        "npm-asset/flag-icons": "7.3.2"
    },
    "config": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm"
        }
    },
    "autoload": {
        "psr-4": {
            "thtmorais\\icons\\": "src"
        }
    }
}
